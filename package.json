{
  "name": "nuxt-music-player",
  "version": "0.0.99",
  "private": true,
  "$schema": "https://json.schemastore.org/package.json",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 delete mitsuki-music-forum",
    "prepare": "husky",
    "precommit": "node ./.husky/scripts/updateVersion.js && git add package.json",
    "prisma:generate": "pnpm prisma generate",
    "prisma:push": "prisma db push",
    "prisma:sync": "pnpm prisma:push && pnpm prisma:generate",
    "prisma:pull": "prisma db pull",
    "prisma:studio": "prisma studio",
    "start:mongodb": "mongod --config E:\\MongoDB\\mongod.cfg"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.0",
    "@prisma/client": "^6.17.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "artplayer": "^5.3.0",
    "axios": "^1.12.2",
    "nuxt": "^3.17.5",
    "pinia": "^3.0.2",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@types/axios": "^0.14.4",
    "@types/node": "^22.15.29",
    "@types/spark-md5": "^3.0.5",
    "autoprefixer": "^10.4.21",
    "husky": "^9.1.7",
    "pm2": "^6.0.13",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.5.3",
    "prisma": "^6.17.0",
    "sass": "^1.89.1"
  }
}
